{
  "name": "place-my-order",
  "version": "0.0.0",
  "description": "",
  "homepage": "",
  "author": {
    "name": "",
    "email": "",
    "url": ""
  },
  "scripts": {
    "api": "place-my-order-api --port 7070",
    "test": "testee src/test.html --browsers firefox --reporter Spec",
    "start": "can-serve --proxy http://localhost:7070 --port 8080",
    "develop": "can-serve --develop --proxy http://localhost:7070 --port 8080",
    "document": "documentjs",
    "build": "node build"
  },
  "main": "production.html",
  "files": [
    "src"
  ],
  "keywords": [],
  "system": {
    "main": "place-my-order/index.stache!done-autorender",
    "directories": {
      "lib": "src"
    },
    "configDependencies": [
      "live-reload"
    ],
    "envs": {
      "server-production": {
        "baseURL": "https://pmo-hurgleburgler.firebaseapp.com/"
      }
    }
  },
  "dependencies": {
    "bit-tabs": "^0.2.0",
    "can": "^2.3.16",
    "can-connect": "^0.3.4",
    "can-ssr": "^0.11.6",
    "done-autorender": "^0.6.3",
    "done-component": "^0.4.0",
    "done-css": "~1.1.16",
    "generator-donejs": "^0.7.0",
    "jquery": "~2.2.1",
    "place-my-order-api": "^0.4.4",
    "place-my-order-assets": "^0.1.7",
    "steal": "^0.14.0",
    "steal-platform": "0.0.4",
    "steal-socket.io": "^1.0.1"
  },
  "devDependencies": {
    "can-fixture": "^0.1.2",
    "documentjs": "^0.4.2",
    "donejs-cli": "^0.7.0",
    "donejs-deploy": "^0.4.0",
    "funcunit": "~3.0.0",
    "steal-cordova": "^0.1.3",
    "steal-nw": "^0.1.4",
    "steal-qunit": "^0.1.1",
    "steal-tools": "^0.14.0",
    "testee": "^0.2.4"
  },
  "window": {
    "width": 1000,
    "height": 600,
    "toolbar": false
  },
  "donejs": {
    "deploy": {
      "root": "dist",
      "services": {
        "production": {
          "type": "firebase",
          "config": {
            "firebase": "pmo-hurgleburgler",
            "public": "./dist",
            "headers": [{
              "source": "/**",
              "headers": [{
                "key": "Access-Control-Allow-Origin",
                "value": "*"
              }]
            }]
          }
        }
      }
    }
  }
}
